<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
 <handlers>
            <add name="PHP fast CGI" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\php\php-cgi.exe" resourceType="Either" requireAccess="Script" />
        </handlers>
        <defaultDocument>
            <files>
                <add value="index.php" />
            </files>
        </defaultDocument>       
    </system.webServer>
    <system.web>
        <authentication mode="Forms" />
    </system.web>
</configuration>
